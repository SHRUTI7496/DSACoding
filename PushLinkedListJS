class LinkedList {
    constructor() {
        this.head = null;
        this.tail = null;
        this.length = 0;
    }

    push(value) {
        const newNode = new Node(value);

        if (!this.head) {
            this.head = newNode;
            this.tail = newNode;
        } else {
            this.tail.next = newNode;
            this.tail = newNode;
        }

        this.length++;
        return this;
    }
}


class Node{
    constructor (value){
        this.value= value;
        this.next= null;
    }
    
}


let  l1=new LinkedList();
l1.push(3)
console.log(l1);
